<constant name="HEIGHT_G_2" value="3*(wire_width+2*coat_thick)"/>
<constant name="WIDTH_G_3" value="10*(wire_width+2*coat_thick)"/>
<constant name="HEIGHT_G_3" value="5*(wire_width+2*coat_thick)"/>
