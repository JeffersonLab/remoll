    <tube aunit="deg" startphi="fstartPhiSideShield1" deltaphi="fdeltaPhiSideShield1" lunit="cm" name="outScattChamberShld_p2" rmin="inner_r_sc_shld" rmax="inner_r_sc_shld+fThickSideSCShield" z="fHightSideSCShield" />
