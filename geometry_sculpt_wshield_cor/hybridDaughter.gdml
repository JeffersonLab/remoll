<constant name="seg_A_radius" value="seg_Z_radius+(seg_length_Z/2)*tan(seg_Z_angle*DEGRAD)+(seg_length_A/2)*tan(seg_A_angle*DEGRAD)-4*(wire_width+2*coat_thick)"/>
