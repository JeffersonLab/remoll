    <box lunit="cm" name="ped_infrac_4_o" x="ped_infrac2_width" y="ped_infrac2_high" z="ped_infrac2_length2"/>
    <box lunit="cm" name="ped_infrac_4_i" x="ped_infrac2_width" y="ped_infrac2_high - 2." z="ped_infrac2_length2 - 1."/>
