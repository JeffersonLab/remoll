    <polyhedra name=  "scattering_chamb_pedestal_p1" startphi="0" deltaphi="360"  numsides="6" aunit="deg" lunit="cm">
      <zplane rmin="0" rmax="sc_arm_radius_in - 5." z="-0.5*sc_arm_ped_high" />
      <zplane rmin="0" rmax="sc_arm_radius_in - 5." z="0.5*sc_arm_ped_high" />
