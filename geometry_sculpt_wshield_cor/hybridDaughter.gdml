<constant name="seg_H_radius" value="seg_G_radius-(seg_length_H/2)*tan(seg_H_angle*DEGRAD)-(seg_length_G/2)*tan(seg_G_angle*DEGRAD)"/>

<constant name="seg_RS_arc_mult" value="5"/>
