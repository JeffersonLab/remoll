    <tube aunit="deg" startphi="0" deltaphi="360" lunit="cm" name="ped_infrac5" rmin="0" rmax="ped_infrac5_tube_radius" z="ped_infrac5_tube_high" />

    <union name ="ped_infrac5_p1">
