    <subtraction name ="outScattChamberShld_p0">
      <first ref="outScattChamberShld_p1"/>
      <second ref="outScattChamberShld_p1_m"/>
      <position name="scShldlayTrans_0" unit="cm" x="0" y="0" z="fHightSideSCShield"/>
      <rotationref ref="identity"/>
