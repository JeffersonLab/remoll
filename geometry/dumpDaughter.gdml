    <tube aunit="deg" startphi="0" deltaphi="360" lunit="cm" name="hut_sc_tube_hut_hole" rmin="0" rmax="1.001*(inner_r_sc_shld+fThickFrontSCShield)" z="height_sc*1.001" />
