<constant name="HEIGHT_C_3" value="5*(wire_width+2*coat_thick)"/>
<constant name="seg_C_radius" value="seg_B_radius+(seg_length_B/2)*tan(seg_B_angle*DEGRAD)+HEIGHT_B_1/2+RADIUS_C_1"/>

