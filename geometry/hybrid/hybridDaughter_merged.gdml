    <subtraction name ="pdscoll_union_2">
	<first ref="pdscoll_union_1"/>
	<second ref="pcons_sculpt_2"/> 
	<rotation name="pdscoll_rot3" x="0" y="0" z="SEPTANT*(2+0.5)" unit="deg"/>
    </subtraction>
    <subtraction name ="pdscoll_union_3">
